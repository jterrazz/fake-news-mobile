{
  "expo": {
    "name": "Fake News",
    "slug": "fake-news",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "developmentClient": {
      "silentLaunch": true
    },
    "jsEngine": "hermes",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jterrazz.fakenews",
      "hermesBuildFlags": ["-DWITH_DYNARR=0"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jterrazz.fakenews"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/splash-icon.png",
          "dark": {
            "image": "./assets/splash-icon-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 256
        }
      ],
      "expo-font"
    ],
    "fonts": [
      {
        "asset": "./assets/fonts/Geist-Thin.otf"
      },
      {
        "asset": "./assets/fonts/Geist-ExtraLight.otf"
      },
      {
        "asset": "./assets/fonts/Geist-Light.otf"
      },
      {
        "asset": "./assets/fonts/Geist-Regular.otf"
      },
      {
        "asset": "./assets/fonts/Geist-Medium.otf"
      },
      {
        "asset": "./assets/fonts/Geist-SemiBold.otf"
      },
      {
        "asset": "./assets/fonts/Geist-Bold.otf"
      },
      {
        "asset": "./assets/fonts/Geist-ExtraBold.otf"
      },
      {
        "asset": "./assets/fonts/Geist-Black.otf"
      }
    ]
  }
}
