{
  "expo": {
    "name": "Fake News",
    "slug": "fake-news",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app-icon.png",
    "userInterfaceStyle": "automatic",
    "developmentClient": {
      "silentLaunch": true
    },
    "jsEngine": "hermes",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jterrazz.fakenews",
      "hermesBuildFlags": ["-DWITH_DYNARR=0"],
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleLocalizations": ["en", "fr"]
      }
    },
    "android": {
      "package": "com.jterrazz.fakenews"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/icons/app-transparent-dark.png",
          "dark": {
            "image": "./assets/icons/app-transparent.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 128,
          "resizeMode": "contain"
        }
      ],
      "expo-font"
    ],
    "fonts": [
      {
        "asset": "./assets/fonts/Geist-Thin.otf"
      },
      {
        "asset": "./assets/fonts/Geist-ExtraLight.otf"
      },
      {
        "asset": "./assets/fonts/Geist-Light.otf"
      },
      {
        "asset": "./assets/fonts/Geist-Regular.otf"
      },
      {
        "asset": "./assets/fonts/Geist-Medium.otf"
      },
      {
        "asset": "./assets/fonts/Geist-SemiBold.otf"
      },
      {
        "asset": "./assets/fonts/Geist-Bold.otf"
      },
      {
        "asset": "./assets/fonts/Geist-ExtraBold.otf"
      },
      {
        "asset": "./assets/fonts/Geist-Black.otf"
      }
    ]
  }
}
